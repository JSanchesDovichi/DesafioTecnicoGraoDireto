    "version": "0.2.0",
    "configurations": [
        {
            "name": "Chromium (web debug)",
            "cwd": "front_end",
            "request": "launch",
            "type": "dart",
            "env": {
                "CHROME_EXECUTABLE": "/usr/bin/chromium"
            },
            "deviceId": "chrome",
            "args": ["--web-hostname", "127.0.0.1", "--web-port", "8027"]
        },
        {
            "name": "Chrome (web debug)",
            "cwd": "front_end",
            "request": "launch",
            "type": "dart",
            "env": {
                "CHROME_EXECUTABLE": "/usr/bin/google-chrome-stable"
            },
            "deviceId": "chrome",
            "args": ["--web-hostname", "127.0.0.1", "--web-port", "8027"]
        },
    ]
}